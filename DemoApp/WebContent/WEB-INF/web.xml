  <!-- MyServlet servlet -->
  <servlet>
  	<servlet-name>myservletservlet</servlet-name>
  	<servlet-class>com.gamehub.MyServlet</servlet-class>
  	
  	<!--  Servlet Config -->
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>RomanG</param-value>
  	</init-param>
  	
  </servlet>
  <servlet-mapping>
  	<servlet-name>myservletservlet</servlet-name>
  	<url-pattern>/myservlet</url-pattern>
  </servlet-mapping>
  
  <!-- Context Parameters -->
  <context-param>
  	<param-name>name</param-name>
  	<param-value>Roman</param-value>
  </context-param>
    <context-param>
  	<param-name>phone</param-name>
  	<param-value>iPhone</param-value>
  </context-param>
  
